{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/client/$1"
        }
    ],
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ]
}